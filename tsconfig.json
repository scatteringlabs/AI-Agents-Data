{
  "compilerOptions": {
    "target": "es5",
    "paths": {
      "@/*": ["./*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/index.tsx", "pages/trade.tsx", "pages/_document.tsx", "next.config.js", "components/tv-chart-container-l", "components/tv-chart-container-l.tsx/MyCustomDataFeed.js"],
  "exclude": ["node_modules"]
}
